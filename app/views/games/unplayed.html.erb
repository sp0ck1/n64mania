

<table>
 <tr>
  <th>ID</th>
  <th>Name</th>
  <th>Genre</th>
 </tr>
 <% Game.unplayed.each do |game| %>
 <% j = game.id %>
 <tr>
  <td><%= j %></td>
  <td><%= link_to game.name, game_path(game) %></td>
  <%= render partial: "genre_links", locals: {game: game} %>
 </tr>
 <% end %>
</table>